
@{
    ViewData["Title"] = "Oops...";
}


<body class="login-body">
    <div class="main-section-forgotp" style="width:800px">
        <header>
            <h1 class="text-danger">@ViewData["Title"]</h1>
            <p class="text-danger" style="margin-bottom: 0px">
                An error has occured, problably other user tried to search at the same time as you.
            </p>
            <p class="text-danger" style="margin-bottom: 0px">Please try again, we sincerely apologize and will work to fix it as soon as possible.</p>
        </header>
    </div>
</body>